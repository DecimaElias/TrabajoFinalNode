'use strict';

/**
 * Module dependencies.
 */

var debug = require('debug')('cnpm');
var pkg = require('../package.json');
var argv = require('../parse_argv')();

var action = argv.args[0];

debug('cnpm %s', action);

debug('origin npm with origin registry: $ npm %s', action);
require('../origin_npm.js');